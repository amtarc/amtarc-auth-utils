{
  "name": "@amtarc/auth-utils",
  "version": "1.3.0",
  "description": "Enterprise-grade authentication and authorization utilities with session management, RBAC, guards, secure cookies, and comprehensive error handling",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./session": {
      "types": "./dist/session/index.d.ts",
      "import": "./dist/session/index.js",
      "require": "./dist/session/index.cjs"
    },
    "./guards": {
      "types": "./dist/guards/index.d.ts",
      "import": "./dist/guards/index.js",
      "require": "./dist/guards/index.cjs"
    },
    "./cookies": {
      "types": "./dist/cookies/index.d.ts",
      "import": "./dist/cookies/index.js",
      "require": "./dist/cookies/index.cjs"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js",
      "require": "./dist/errors/index.cjs"
    },
    "./security": {
      "types": "./dist/security/index.d.ts",
      "import": "./dist/security/index.js",
      "require": "./dist/security/index.cjs"
    },
    "./security/csrf": {
      "types": "./dist/security/csrf/index.d.ts",
      "import": "./dist/security/csrf/index.js",
      "require": "./dist/security/csrf/index.cjs"
    },
    "./security/rate-limit": {
      "types": "./dist/security/rate-limit/index.d.ts",
      "import": "./dist/security/rate-limit/index.js",
      "require": "./dist/security/rate-limit/index.cjs"
    },
    "./security/headers": {
      "types": "./dist/security/headers/index.d.ts",
      "import": "./dist/security/headers/index.js",
      "require": "./dist/security/headers/index.cjs"
    },
    "./security/encryption": {
      "types": "./dist/security/encryption/index.d.ts",
      "import": "./dist/security/encryption/index.js",
      "require": "./dist/security/encryption/index.cjs"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js",
      "require": "./dist/storage/index.cjs"
    },
    "./authorization": {
      "types": "./dist/authorization/index.d.ts",
      "import": "./dist/authorization/index.js",
      "require": "./dist/authorization/index.cjs"
    },
    "./authorization/rbac": {
      "types": "./dist/authorization/rbac/index.d.ts",
      "import": "./dist/authorization/rbac/index.js",
      "require": "./dist/authorization/rbac/index.cjs"
    },
    "./authorization/types": {
      "types": "./dist/authorization/types.d.ts",
      "import": "./dist/authorization/types.js",
      "require": "./dist/authorization/types.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "authentication",
    "authorization",
    "session",
    "session-management",
    "security",
    "guards",
    "cookies",
    "csrf",
    "rate-limiting",
    "brute-force-protection",
    "security-headers",
    "csp",
    "encryption",
    "aes-256-gcm",
    "key-derivation",
    "fingerprinting",
    "multi-device",
    "typescript",
    "framework-agnostic"
  ],
  "author": "amtarc",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@vitest/coverage-v8": "^1.3.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
